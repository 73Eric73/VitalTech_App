{
  "name": "hospital-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm install && concurrently \"nx serve VitalTech  --ssl -o --host=127.0.0.1\" \"nx serve GoldenFold --ssl -o --host=127.0.0.2\"",
    "ultraStart": "npm install && concurrently \"nx serve VitalTech --ssl -o --host=127.0.0.1\" \"nx serve GoldenFold --ssl -o --host=127.0.0.2\" \"dotnet watch --project ../HospitalApi/HospitalApi.csproj run\"",
    "build": "nx build",
    "watch": "nx build --watch --configuration development",
    "test": "nx test",
    "serve:trace": "powershell -Command \"$env:NODE_OPTIONS='--trace-warnings'; nx serve VitalTech --ssl\""  
  },
  "projects": {
    "VitalTech": {
      "root": "apps/VitalTech",
      "sourceRoot": "apps/VitalTech/src",
      "projectType": "application",
      "architect": {
        "build": {
          "options": {
            "outputPath": "dist/apps/VitalTech"
          }
        },
        "serve": {
          "options": {
            "browserTarget": "VitalTech:build"
          }
        }
      }
    },
    "GoldenFold": {
      "root": "apps/GoldenFold",
      "sourceRoot": "apps/GoldenFold/src",
      "projectType": "application",
      "architect": {
        "build": {
          "options": {
            "outputPath": "dist/apps/GoldenFold"
          }
        },
        "serve": {
          "options": {
            "browserTarget": "GoldenFold:build"
          }
        }
      }
    },
    "API": {
      "root": "../HospitalApi",
      "sourceRoot": "../HospitalApi/src",
      "projectType": "application",
      "architect": {
        "build": {
          "options": {
            "outputPath": "dist/apps/API"
          }
        },
        "serve": {
          "options": {
            "browserTarget": ""
          }
        }
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "angular-auth-oidc-client": "^18.0.2",
    "chart.js": "^4.4.3",
    "fuse.js": "^7.0.0",
    "jest-editor-support": "*",
    "keycloak-angular": "^16.0.1",
    "keycloak-js": "^25.0.5",
    "ng2-charts": "^6.0.1",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.14.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.6",
    "@angular-devkit/core": "^18.2.3",
    "@angular-devkit/schematics": "^18.2.3",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.0.0",
    "@nx/angular": "20.2.1",
    "@nx/js": "20.2.1",
    "@nx/workspace": "20.2.1",
    "@schematics/angular": "^18.2.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.8.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nx": "20.2.1",
    "prettier": "^2.6.2",
    "typescript": "~5.4.2"
  }
}
